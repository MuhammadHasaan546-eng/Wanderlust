<% layout('/layouts/boilerplate.ejs') %>

<div class="row">
  <div
    class="alert alert-danger alert-dismissible fade show col-8 offset-2"
    role="alert"
  >
    <p class="alert-header"><%= err.message %></p>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
</div>
